    <!-- Modal Overlay -->
    <div class="modal-overlay" id="modalOverlay">
        <div class="modal">
            <div class="modal-header">
                <div class="modal-title">
                    <span>üìã</span>
                    <span>Client Consent Form</span>
                </div>
                <button class="modal-close" onclick="closeModal()">
                    <i class="fas fa-times"></i>
                </button>
            </div>
            
            <div class="modal-content">
                <!-- Step 1: Client Details Form -->
                <div class="workflow-step active" id="step1">
                    <div class="step-header">
                        <div class="step-number">1</div>
                        <div class="step-title">Client Details</div>
                        <div class="step-description">Enter your client's information to auto-populate the consent form</div>
                    </div>

                    <div class="client-form">
                        <div class="form-grid">
                            <div class="form-group">
                                <label class="form-label">Full Name *</label>
